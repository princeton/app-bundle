{
	"description": "The Princeton app bundle provides an application bundle built on top of Slim.",
	"name": "princeton/app-bundle",
	"version": "0.1.5",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Kevin Perry",
			"email": "perry@princeton.edu",
			"role": "Lead Developer"
		}
	],
	"support": {
		"email": "perry@princeton.edu"
	},
	"require": {
		"php": ">=5.4.23",
		"ext-json": ">=1.2.1",
		"symfony/yaml": "~2.4"
	},
	"require-dev": {
		"doctrine/mongodb": "~1.0",
		"doctrine/mongodb-odm": "~1.0@dev",
		"slim/slim": "~2.4",
		"slim/views": ">=0.1.1",
		"twig/twig": "~1.15",
		"jasig/phpcas": "~1.3",
		"phpstubs/phpstubs": "dev-master",
		"google/apiclient": "dev-master",
		"php-ews/php-ews": "dev-master",
        "phpunit/phpunit-skeleton-generator": "~2.0",
		"phpunit/phpunit": "~4.4"
	},
	"suggest": {
		"doctrine/mongodb": "~1.0",
		"doctrine/mongodb-odm": "~1.0@dev",
		"slim/slim": "~2.4",
		"slim/views": ">=0.1.1",
		"twig/twig": "~1.15",
		"jasig/phpcas": "dev-master",
		"google/apiclient": "dev-master",
		"php-ews/php-ews": "dev-master"
	},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/princeton/phpunit-skeleton-generator"
        }
    ],
	"autoload": {
		"psr-4": {
			"Princeton\\App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\": "tests/"
		}
	}
}
